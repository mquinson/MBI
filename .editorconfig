# MBI editor config

root = true

[**]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false
max_line_length = 80

[**.py]
charset = utf-8
indent_style = space
indent_size = 4
tab_width = 2

[**.{c,cpp,h,hpp}]
charset = utf-8
indent_style = space
indent_size = 2
tab_width = 2
